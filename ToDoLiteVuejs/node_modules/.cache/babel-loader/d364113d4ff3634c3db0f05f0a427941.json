M,CAAf","sourcesContent":["<template>\n  <div class=\"container\">\n    <h1 id=\"title\">Inscription</h1>\n    <br />\n    <br />\n    <div class=\"row\">\n      <!-- La partie droite  -->\n      <div class=\"col-md-3\"></div>\n      <div class=\"col-md-6\">\n\n        <form>\n\n          <!-- user -->\n          <div class=\"form-group\">\n              <label for=\"email\">Nom  :</label>\n            <input\n              type=\"text\"\n              class=\"form-control\"\n              id=\"nom\"\n              required\n              v-model=\"user\"\n              placeholder=\"Entrer votre nom \"\n            />\n          </div>\n          <br />\n\n          <!-- email -->\n          <div class=\"form-group\">\n           <label for=\"email\">Email :</label>\n            <input\n              type=\"email\"\n              class=\"form-control\"\n              id=\"email\"\n              required\n              v-model=\"email\"\n              aria-describedby=\"emailHelp\"\n              placeholder=\"Entrer votre adresse email\"\n            />\n          </div>\n\n          <br />\n          <!-- mot de passe -->\n          <div class=\"form-group\">\n              <label for=\"password\">Password :</label>\n            <input\n              type=\"password\"\n              class=\"form-control\"\n              id=\"motDePasse\"\n              required\n              v-model=\"motDePasse\"\n              placeholder=\"Mot de passe\"\n            />\n          </div>\n\n          <div\n            v-if=\"motDePasse.length > 1 && motDePasse.length < 8\"\n            class=\"text-danger\"\n          >\n            Le mot de passe doit contenir minimum 8 caractères.\n          </div>\n          <br />\n\n          <!-- confirmation mot de passe -->\n          <div class=\"form-group\">\n              <label for=\"cfr_motDePasse\">Password confirmation :</label>\n            <input\n              type=\"password\"\n              class=\"form-control\"\n              id=\"cfr_motDePasse\"\n              required\n              v-model=\"cfr_motDePasse\"\n              placeholder=\"Confirmez votre mot de passe\"\n            />\n          </div>\n\n          <!-- controle de la correspondance des deux mots de passe  -->\n          <div v-if=\"motDePasse != cfr_motDePasse\" class=\"text-danger\">\n            Mot de passe incorrect.\n          </div>\n          <br />\n\n          <button  @click=\"inscription\" type=\"submit\" class=\"btn btn-primary\">S'inscrire</button>\n\n          <br>\n          <button @click=\"loadUsers\">Afficher les données</button>\n          <p>Liste: {{ users }}</p>\n\n        </form>\n      </div>\n      <!-- La partie droite  -->\n      <div class=\"col-md-3\"></div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport { mapActions } from 'vuex';\n\nexport default {\n  namespaced: true,\n  name: \"Inscription\",\n  data() {\n    return {\n      token: '',\n      users: [],\n      user: \"\",\n      email: \"\",\n      motDePasse: \"\",\n      cfr_motDePasse: \"\"\n    };\n  },\n  methods: {\n\n    ...mapActions(\n       \"compte\", [\"signup\"] //module, method\n    ),\n    mounted(){\n      this.signup();\n    },\n\n    inscription(){\n      if(this.motDePasse === this.cfr_motDePasse){\n        this.signup({'user': this.user, 'email': this.email, 'password': this.motDePasse}).then( function(response) {\n          this.token = response.data;\n          console.log(response);\n      })\n      .catch(error => {\n        console.log(error);\n      });\n      }  \n    },\n    \n    loadUsers() {\n      axios.get(\"https://api.github.com/users/mapbox\").then(\n        function(response) {\n          this.users = response.data;\n        }.bind(this)  \n      );\n    }\n  }\n};\n</script>\n\n<style></style>"],"sourceRoot":""}]}